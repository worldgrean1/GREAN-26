{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "@sandbox/GREAN-25",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:production": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "clean": "rm -rf .next out node_modules/.cache",
    "postbuild": "echo 'Build completed successfully'",
    "netlify:build": "npm run build",
    "netlify:dev": "netlify dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "chunk:check": "node scripts/fix-chunk-loading.js check",
    "chunk:clean": "node scripts/fix-chunk-loading.js clean",
    "chunk:rebuild": "node scripts/fix-chunk-loading.js rebuild",
    "chunk:fix": "node scripts/fix-chunk-loading.js fix",
    "dev:clean": "npm run chunk:clean && npm run dev",
    "build:clean": "npm run chunk:clean && npm run build",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build:debug": "cross-env DEBUG=1 npm run build"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
